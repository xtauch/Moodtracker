<?xml version="1.0" encoding="utf-8"?>
<FrameLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.example.youpiman.moodtracker.model.VerticalViewPager

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/pager"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    </com.example.youpiman.moodtracker.model.VerticalViewPager>

    <ImageButton
        android:id="@+id/note"
        android:contentDescription="@string/note_add_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginBottom="18dp"
        android:background="@mipmap/ic_note_add_black" />


    <ImageButton
        android:id="@+id/history"
        android:contentDescription="@string/history_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginBottom="20dp"
        android:background="@mipmap/ic_history_black"
       />

</FrameLayout>
